{
  "name": "nodejs-basic",
  "version": "1.0.0",
  "description": "This is a basic Node.js application that demonstrates how to create a simple server using Node.js.",
  "main": "index.js",
  "type": "commonjs",
  "dependencies": {
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "ejs": "^3.1.9",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "delay": "1000",
    "verbose": true,
    "env": {
      "PORT": 4000
    }
  },
  "scripts": {
    "start": "DEBUG=app nodemon node app.js",
    "debug": "DEBUG=* nodemon node app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
